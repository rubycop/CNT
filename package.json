    "@near-wallet-selector/sender": "^7.4.0",
    "@near-wallet-selector/wallet-connect": "^7.4.0",
    "@prisma/client": "^4.5.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "better-react-carousel": "^1.1.1",
    "big.js": "^6.2.1",
    "cors": "^2.8.5",
