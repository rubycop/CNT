{
  "name": "bmt",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:contracts": "(cd contracts && ./build.sh)",
    "local:build:web": "NODE_ENV=local parcel build src/index.html --public-url ./",
    "dev:build:web": "NODE_ENV=development parcel build src/index.html --public-url ./",
    "local:deploy:clear": "rm -rf ./neardev",
    "local:deploy:contract": "yarn build:contracts && ./local-deploy.sh",
    "dev:deploy:contract": "NEAR_ENV=testnet near deploy --accountId \"contesty-v2.testnet\" --wasmFile out/main.wasm",
    "prod:deploy:contract": "NEAR_ENV=mainnet near deploy --accountId \"beat_my_nft.near\" --wasmFile out/main.wasm",
    "prestart": "rm -rf ./parcel-cache",
    "cleanup": "rm -rf .parcel-cache && rm -rf ./dist",
    "start": "yarn cleanup && NODE_ENV=local parcel src/index.html --open",
    "local:server": "env $(cat .env.local) nodemon src/server/server.js --watch src/server",
    "prod:server:restart": "env $(cat .env.production) pm2 restart ./src/server/server.js --name contesty-mainnet --update-env",
    "dev:server:restart": "env $(cat .env.development) pm2 restart ./src/server/server.js --name contesty-testnet --update-env"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "near-cli": "^3.4.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.14",
    "querystring-es3": "^0.2.1",
    "shelljs": "~0.8.5",
    "tailwindcss": "^3.1.0",
    "url": "^0.11.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@near-wallet-selector/coin98-wallet": "^7.4.0",
    "@near-wallet-selector/core": "^7.4.0",
    "@near-wallet-selector/default-wallets": "^7.4.0",
    "@near-wallet-selector/here-wallet": "^7.4.0",
    "@near-wallet-selector/ledger": "^7.4.0",
    "@near-wallet-selector/math-wallet": "^7.4.0",
    "@near-wallet-selector/meteor-wallet": "^7.4.0",
    "@near-wallet-selector/modal-ui": "^7.4.0",
    "@near-wallet-selector/my-near-wallet": "^7.4.0",
    "@near-wallet-selector/near-wallet": "^7.4.0",
    "@near-wallet-selector/nearfi": "^7.4.0",
    "@near-wallet-selector/nightly": "^7.4.0",
